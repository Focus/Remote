<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	android:versionCode="1" android:versionName="1.0" package="com.focus.remote">
	<uses-sdk android:minSdkVersion="7" />

	<application android:name=".TCPConnect" android:icon="@drawable/icon"
		android:label="@string/app_name">
		<activity android:name=".RemoteXActivity" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".Ipscreen" android:label="@string/ipscreen"
			android:noHistory="true" />
		<activity android:name=".MouseScreen" android:label="@string/mainscreen" />
		<activity android:name=".Instruct" android:label="@string/instructs" />
		<activity android:name=".Preferences" android:label="@string/preferences" />
		<activity android:name=".RemoteSelector" android:label="@string/remoteselector" />
		<activity android:name=".RemoteScreen" android:noHistory="true" />
		<activity android:name=".RemoteEditor" android:noHistory="true" />

	</application>
	<uses-permission android:name="android.permission.INTERNET" />
</manifest>